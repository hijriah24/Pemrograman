# Pemrograman
A = 20
B = 15
print (A + B) 
print (A - B) 
print (A * B) 
print (A / B) 

#penjumlahan
n = 20
r = 15
penjumlahan = n + r
print("Hasil penjumlahan:", penjumlahan) 

#pengurangan
n = 60
r = 12
pengurangan = n - r
print("Hasil pengurangan", pengurangan) 

#perkalian
n = 80
r = 25
perkalian = n - r
print("Hasil perkalian", perkalian) 

#pembagian
n = 90
r = 25
pembagian = n / r
print(" Hasil pembagian", pembagian) 
